{
  "name": "blockly-vue3",
  "version": "0.0.0",
  "keywords": [
    "vue",
    "phaser",
    "vite",
    "boilerplate",
    "template"
  ],
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev-watch": "nodemon server.js",
    "build": "vue-tsc --noEmit && vite build",
    "prodBuild": "vue-tsc --noEmit && node server.js && vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "acorn": "^8.8.2",
    "blockly": "^9.3.2",
    "express": "^4.18.2",
    "phaser": "^3.60.0",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "vue": "^3.2.37",
    "vue-router": "^4.2.2",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.22",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.6.0",
    "vue-tsc": "^1.2.0"
  }
}
